<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RevisionPlanner.View.SelectQualificationPage"
             Title="Qualification">
    <VerticalStackLayout>
        <Button Text="GCSE"
                Command="{Binding GcseCommand}" />
        <Button Text="A-Level"
                Command="{Binding ALevelCommand}" />
        <Button Text="Other"
                Command="{Binding OtherCommand}" />
    </VerticalStackLayout>
</ContentPage>
